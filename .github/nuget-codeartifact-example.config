<?xml version="1.0" encoding="utf-8"?>
<!--
  Example NuGet.config for consuming ThirdOpinion.Common from AWS CodeArtifact

  To use this configuration:
  1. Configure AWS CLI with appropriate credentials
  2. Run: aws codeartifact login --tool dotnet --domain YOUR_DOMAIN --repository YOUR_REPOSITORY
  3. Copy this file to your project as nuget.config
  4. Replace YOUR_DOMAIN, YOUR_ACCOUNT_ID, and YOUR_REPOSITORY with actual values
-->
<configuration>
  <packageSources>
    <add key="aws-codeartifact" value="https://YOUR_DOMAIN-YOUR_ACCOUNT_ID.d.codeartifact.us-east-1.amazonaws.com/nuget/YOUR_REPOSITORY/v3/index.json" />
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
  </packageSources>
</configuration>