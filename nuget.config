<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <packageSources>
        <clear/>
        <add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3"/>
    </packageSources>

    <!-- AWS CodeArtifact source will be added dynamically by the publish script -->
    <!-- The source name 'aws-codeartifact' is used by the publish script -->

    <packageSourceMapping>
        <packageSource key="nuget.org">
            <package pattern="*"/>
        </packageSource>
    </packageSourceMapping>
</configuration>